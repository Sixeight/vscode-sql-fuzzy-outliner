{
  "name": "sql-fuzzy-outliner",
  "displayName": "SQL Fuzzy Outliner",
  "description": "A fuzzy outliner for SQL and GraphQL with definition/reference jump support",
  "publisher": "Sixeight",
  "homepage": "https://github.com/Sixeight/sql-fuzzy-outliner",
  "repository": {
    "type": "git",
    "url": "https://github.com/Sixeight/sql-fuzzy-outliner.git"
  },
  "version": "0.4.0",
  "engines": {
    "vscode": "^1.96.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "keywords": [
    "sql",
    "graphql",
    "outline",
    "navigation",
    "definition"
  ],
  "activationEvents": [
    "onLanguage:sql",
    "onLanguage:graphql"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "languages": [
      {
        "id": "graphql",
        "extensions": [
          ".graphql",
          ".gql",
          ".graphqls"
        ],
        "aliases": [
          "GraphQL",
          "graphql"
        ]
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "pnpm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "pnpm run compile && pnpm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "dependencies": {
    "graphql": "^16.7.0"
  },
  "devDependencies": {
    "@types/vscode": "^1.97.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@typescript-eslint/eslint-plugin": "^8.22.0",
    "@typescript-eslint/parser": "^8.22.0",
    "eslint": "^9.19.0",
    "typescript": "^5.7.3",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1"
  },
  "packageManager": "pnpm@9.15.4+sha512.b2dc20e2fc72b3e18848459b37359a32064663e5627a51e4c74b2c29dd8e8e0491483c3abb40789cfd578bf362fb6ba8261b05f0387d76792ed6e23ea3b1b6a0"
}
